//SYSIN    DD   *                                                               
RCTE3 DBFRCT  FPSAMP1,DBFSAMP3,10                                               
RCTE1 DBFRCT  IVTFD,DFSIVP4,10                                                  
RCTE2 DBFRCT  IVTFM,DFSIVP5,10                                                  
         DBFRCT  GEN                                                            
         END                                                                    
//STEP7    EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSCLL0I)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
CLL0     TITLE 'DFSICLL0 - IMS COMMUNICATION CONTROL BLOCKS, MODULE 0.'         
         GBLB  &DFBESEC   ENHANCED SECURITY SPECIFIED                           
&DFBESEC SETB  0      FOR EXTENDED CTB                                          
         DFSCLB B,1,0,0,1,000800,0,CT0,0,0 SYSTEM CONSOLE CLB.                  
.DFCL1  DFSCLB B,2,DFDCB1,0,1,0008,0,CT1,0,0                                    
         AGO   .DFCL2                                                           
.DFPL1  ANOP                                                                    
         AGO   .DFPL2                                                           
.LERB1 ANOP                                                                     
         AGO   .LERB2                                                           
.DFCL2  DFSCLB B,3,DFDCB2,0,1,00E800,0,CT2,0,3                                  
         ENTRY DFSTFCLB                                                         
DFSTFCLB DFSCLB B,4,0,0,0,000000,0,DFSTFCTB,0,0                                 
         AGO   .DFLL4                                                           
.DFPL0   ANOP                                                                   
         TITLE 'DFSICLL0 - COMMUNICATION LINE POLLING LISTS.'                   
         AGO   .DFPL1                                                           
.DFPL2  ANOP                                                                    
         TITLE 'DFSICLL0 - COMMUNICATION LINE ERROR BLOCKS.'                    
         ENTRY DFSMSCLB                                                         
DFSMSCLB DS    0F                                                               
         AGO   .LERB1                                                           
.DFLL4   ANOP                                                                   
         ENTRY DFSMTCLB                                                         
DFSMTCLB DFSCLB B,0,0,0,0,000000,0,DFSMTCTB,0,0                                 
         ENTRY DFSRMCLB                                                         
DFSRMCLB DFSCLB B,00,0,0,0,000000,0,DFSRMCTB,0,0                                
         ENTRY DFSWTCLB                                                         
DFSWTCLB DFSCLB B,5,0,0,1,000800,0,DFSWTCTB,0,0                                 
         ENTRY DFSPMCLB                                                         
DFSPMCLB DFSCLB V,6,DFSPMCLB,DFSPMCTB,0                                         
         ENTRY DFSSMCLB                                                         
DFSSMCLB DFSCLB B,7,DFDCB1,0,1,0008,0,DFSSMCTB,0,0                              
         AGO   .DFPL0                                                           
.LERB2 ANOP                                                                     
.LERB3   ANOP                                                                   
         TITLE 'DFSICLL0 - COMMUNICATION VTAM ACB.'                             
         ENTRY DFSVTACB                                                         
DFSVAPPL DC    AL1(8),CL8'IVP31CR1'                                             
         ENTRY DFSVAPPL                                                         
DFSVPSWD DC    AL1(8),CL8'IVP31CR1'                                             
         EXTRN DFSCNXA0                                                         
DFSVTACB ACB   AM=VTAM,EXLST=DFSCNXA0,APPLID=DFSVAPPL,PASSWD=DFSVPSWD           
         TITLE 'DFSICLL0 - COMMUNICATION LINE GROUP DCB''S.'                    
DFDCB1 DCB   DSORG=PS,MACRF=W,DEVD=DA,                                 ;        
               DDNAME=IVPPRT1                                                   
         EJECT                                                                  
         EXTRN DFSSYNAD                                                         
DFDCB2 DCB   DSORG=PS,MACRF=(R,W),DEVD=DA,                             ;        
               SYNAD=DFSSYNAD,EODAD=DFSSYNAD,RECFM=UM,                 ;        
               DDNAME=IVPSPL1                                                   
         EJECT                                                                  
DFDCB3 DCB   DSORG=PS,MACRF=(R,W),DEVD=DA,                             ;        
               SYNAD=DFSSYNAD,EODAD=DFSSYNAD,RECFM=UM,                 ;        
               DDNAME=IVPSPL2                                                   
         EJECT                                                                  
DFDCB4 DCB   DSORG=PS,MACRF=(R,W),DEVD=DA,                             ;        
               SYNAD=DFSSYNAD,EODAD=DFSSYNAD,RECFM=UM,                 ;        
               DDNAME=IVPSPL3                                                   
         DC    A(0)     END OF SPOOL DCB SET                                    
         TITLE 'DFSICLL0 - COMMUNICATION LINE GROUP OPEN LIST.'                 
         ENTRY DFSICDB                                                          
DFSICDB  DS    0D                                                               
         DC    AL1(15),AL3(DFDCB1)                                              
         DC    AL1(3),AL3(DFDCB2)                                               
         DC    AL1(3),AL3(DFDCB3)                                               
         DC    AL1(131),AL3(DFDCB4)                                             
         EJECT                                                                  
         ENTRY DFSICDCB                                                         
DFSICDCB IDCBOS OPTCD=P,DDNAME=IMSSPA                                           
         SPACE 2                                                                
         ENTRY DFSLIST0                                                         
DFSLIST0 DS    0D            USER SUPPLIED SMB EDITS                            
CT0      DFSCTB B,CTT0,DFSICLB,1,000000,00000B00,00,00,00000000,       ;        
               CN11,0,0,0,00,00,00                                              
CT1     DFSCTB B,CTT1,DFSICLB+172,2,008181,40000B40,00,00,             ;        
               00000000,CN1,0,0,0,00,00,00                                      
CT2     DFSCTB B,CTT2,DFSICLB+344,3,000000,00000B00,C0,00,             ;        
               00000000,CN3,0,0,0,00,00,00                                      
         ENTRY DFSTFCTB                                                         
DFSTFCTB DFSCTB B,DFSTFCTT,DFSTFCLB,4,000000,00000B00,00,00,00000000,  ;        
               DFSTCFI,0,0,0,00,00,00                                           
         ENTRY DFSMTCTB                                                         
DFSMTCTB DFSCTB B,CTT0,DFSMTCLB,0,000000,00000B00,00,00,00000000,      ;        
               DFSMTCNT,0,0,0,00,00,00                                          
         ENTRY DFSRMCTB                                                         
DFSRMCTB DFSCTB B,CTT0,DFSRMCLB,0,000000,00000B00,00,00,00000000,      ;        
               DFSRMCNT,0,0,0,00,00,00                                          
         ENTRY DFSWTCTB                                                         
DFSWTCTB DFSCTB B,CTT0,DFSWTCLB,5,000000,00000B00,00,00,00000000,      ;        
               DFSWTCNT,0,0,0,00,00,00                                          
         ENTRY DFSPMCTB                                                         
DFSPMCTB DFSCTB VDUM,CTT3,DFSPMCLB,6,000000,40000B00,08,2A,            ;        
               00000000,DFSPMCNT,0,0,0,00,00,00                                 
         ENTRY DFSSMCTB                                                         
DFSSMCTB DFSCTB B,CTT1,DFSSMCLB,7,008181,40000B40,00,00,               ;        
               00000000,DFSSMCNT,0,0,0,00,00,00                                 
         ENTRY DFSISID                                                          
DFSISID  DS    0F                                                               
         ENTRY DFSMTCNT                                                         
DFSMTCNT DFSCNT DFSMTCNT,DFSMTCTB,0,001005,0,0,1,0                              
         ENTRY  DFSRMCNT                                                        
DFSRMCNT DFSCNT DFSRMCNT,DFSRMCTB,0,001005,0,0,0,0                              
DFSTCF   DFSCNT DFSTCF,DFSTFCTB,0,000001,0,0,0,0                                
DFSTCFI  DFSCNT DFSTCFI,0,DFSTCF,000001,0,0,0,0                                 
CN1      DFSCNT HM31T021,CT1,CN2,400001,0,0,1,0                                 
         ENTRY CN1                                                              
CN4      DFSCNT HM31T022,0,0,400001,0,0,1,HM31T022                              
         ENTRY CN4                                                              
CN6      DFSCNT HOWARD,0,0,000001,0,0,0,USER1                                   
CN2      DFSCNT IVPPRT1,CT1,0,000001,0,0,1,0                                    
CN3      DFSCNT IVPSPL1,CT2,0,000001,0,0,0,0                                    
CN8      DFSCNT TPNS0001,0,0,000001,0,0,0,TPNS0001                              
CN5      DFSCNT USER1,0,CN6,000001,0,0,0,USER1                                  
CN7      DFSCNT USER2,0,0,000001,0,0,0,USER2                                    
CN11     DFSCNT WTOR,CT0,0,001001,0,0,1,0                                       
         ENTRY DFSWTCNT                                                         
DFSWTCNT DFSCNT DFSWTCNT,DFSWTCTB,0,001001,0,0,0,0                              
DFSPMCNT DFSCNT DFSCN1,DFSPMCTB,0,400001,0,0,0,0                                
         ENTRY DFSPMCNT                                                         
DFSSMCNT DFSCNT DFSCN2,DFSSMCTB,DFSCN3,400001,0,0,0,0                           
         ENTRY DFSSMCNT                                                         
DFSCN3   DFSCNT DFSCN3,DFSSMCTB,0,000001,0,0,0,0                                
         DFSCNT GEN,HM31T022,HM31T021                                           
         END                                                                    
