  ORDER DFSCIOE0,DFSCIOF0,DFSCIOG0                                              
 INCLUDE SYSPUNCH(DFSICIO0)   COMMUNICATIONS ANALYZER.                          
  ORDER DBFHIEL0,DBFHRTR0,DBFHQMI0                                              
  INCLUDE LOAD(DBFHIEL0)    FAST PATH INPUT LINKAGE.                            
  INCLUDE LOAD(DBFHRTR0) FAST PATH COMM. OUTPUT ROUTER                          
  INCLUDE LOAD(DBFHQMI0)    FAST PATH Q-MGR INTERFACE                           
  ORDER DFSCIOA0                                                                
  ORDER DFSHCLG0                                                                
  ORDER DFSHCSW0                                                                
  ORDER DFSHVIO0                                                                
 INCLUDE SYSPUNCH(DFSCIOA0) VTAM RECEIVE ANY HANDLER.                           
 INCLUDE LOAD(DFSHCLG0)     HARP SESS MISC LOG INTERF.                          
 INCLUDE LOAD(DFSHCSW0)     HARP SESS SWITCH CONTROL.                           
 INCLUDE LOAD(DFSHVIO0)     HARP SESSION VIO.                                   
