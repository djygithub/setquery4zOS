//     UNIT=SYSDA,VOL=SER=VOLBA0                                                
//DSNUPROC.SYSUT1 DD DSN=&&SYSUT1,                                              
//     DISP=(,DELETE),DCB=BLKSIZE=15400,                                        
//     SPACE=(CYL,(1000,50),,,ROUND),                                           
//     UNIT=SYSDA,VOL=SER=VOLBA1                                                
//DSNUPROC.SORTOUT DD DSN=&&SORTOT,                                             
