           COMPUTE ABAC2N5 IN OUTPUT-87 = BAC2N5(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC2N6 IN OUTPUT-87 = BAC2N6(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC2N7 IN OUTPUT-87 = BAC2N7(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC2N8 IN OUTPUT-87 = BAC2N8(S-SUB) / NUMT(S-SUB).          
      *                                                                         
           MOVE BAC3N1(S-SUB) TO ABAC3N1 IN OUTPUT-88.                          
           COMPUTE ABAC3N2 IN OUTPUT-88 = BAC3N2(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N3 IN OUTPUT-88 = BAC3N3(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N4 IN OUTPUT-88 = BAC3N4(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N5 IN OUTPUT-89 = BAC3N5(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N6 IN OUTPUT-89 = BAC3N6(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N7 IN OUTPUT-89 = BAC3N7(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC3N8 IN OUTPUT-89 = BAC3N8(S-SUB) / NUMT(S-SUB).          
      *                                                                         
           MOVE BAC4N1(S-SUB) TO ABAC4N1 IN OUTPUT-90.                          
           COMPUTE ABAC4N2 IN OUTPUT-90 = BAC4N2(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N3 IN OUTPUT-90 = BAC4N3(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N4 IN OUTPUT-90 = BAC4N4(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N5 IN OUTPUT-91 = BAC4N5(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N6 IN OUTPUT-91 = BAC4N6(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N7 IN OUTPUT-91 = BAC4N7(S-SUB) / NUMT(S-SUB).          
           COMPUTE ABAC4N8 IN OUTPUT-91 = BAC4N8(S-SUB) / NUMT(S-SUB).          
      *                                                                         
           COMPUTE ATXAN1  IN OUTPUT-92 = TXAN1(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN2  IN OUTPUT-92 = TXAN2(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN3  IN OUTPUT-92 = TXAN3(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN4  IN OUTPUT-92 = TXAN4(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN5  IN OUTPUT-93 = TXAN5(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN6  IN OUTPUT-93 = TXAN6(S-SUB) / NUMT(S-SUB).           
           COMPUTE ATXAN10 IN OUTPUT-94 = TXAN10(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN11 IN OUTPUT-94 = TXAN11(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN12 IN OUTPUT-94 = TXAN12(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN13 IN OUTPUT-94 = TXAN13(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN14 IN OUTPUT-95 = TXAN14(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN15 IN OUTPUT-95 = TXAN15(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN16 IN OUTPUT-95 = TXAN16(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN17 IN OUTPUT-95 = TXAN17(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN18 IN OUTPUT-96 = TXAN18(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN19 IN OUTPUT-96 = TXAN19(S-SUB) / NUMT(S-SUB).          
           COMPUTE ATXAN20 IN OUTPUT-96 = TXAN20(S-SUB) / NUMT(S-SUB).          
      *(3)                                                                      
           WRITE OUTPUT-RECORD FROM OUTPUT-700.                                 
           WRITE OUTPUT-RECORD FROM OUTPUT-701.                                 
           WRITE OUTPUT-RECORD FROM OUTPUT-71.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-72.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-73.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-74.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-75.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-76.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-77.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-78.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-79.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-80.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-81.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-82.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-83.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-84.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-85.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-86.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-87.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-88.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-89.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-90.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-91.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-92.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-93.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-94.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-95.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-96.                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-ASTERISKS.                           
           ADD 1 TO S-SUB.                                                      
      ******************************************************************        
      *                                                                         
      *    NOTE - THIS ROUTINE RETURNS GREENWICH MEAN TIME, WHICH IS            
      *           NOT NECESSARILY LOCAL TIME.  THIS IS A STCK                   
      *           LIMITATION (FEATURE?)                                         
      *                                                                         
      *                                                                         
      *    (1)  INITIALIZE BASE DATE TO 00.00 HOURS, JANUARY 1, 1988            
      *                                                                         
      *    (2)  COMPUTE TIME BETWEEN NOW AND BASE DATE                          
      *                                                                         
      *    (3)  COMPUTE # YEARS BETWEEN BASE DATE AND NOW                       
      *                                                                         
      *    (4)  ADD # YEARS TO BASE                                             
      *                                                                         
      *    (5)  COMPUTE JULIAN DAY OF YEAR                                      
      *                                                                         
      *    (6)  COMPUTE HOURS AND MINUTES                                       
      *                                                                         
      *    (7)  COMPUTE SECONDS                                                 
      *                                                                         
      ******************************************************************        
       CONVERT-TIME-STAMP.                                                      
      *(1)                                                                      
           MOVE 1988 TO YEAR.                                                   
      *(2)                                                                      
           SUBTRACT 1988-YEAR FROM TIME-WORK GIVING TIME-WORK-1.                
      *(3)                                                                      
           DIVIDE TIME-WORK-1 BY 1461-DAYS GIVING TIME-WORK-2                   
               REMAINDER TIME-WORK-3.                                           
           IF  TIME-WORK-2 > 0                                                  
               COMPUTE YEAR = (YEAR + (4 * TIME-WORK-2)).                       
           DIVIDE TIME-WORK-3 BY 365-DAYS GIVING TIME-WORK-4                    
               REMAINDER TIME-WORK-5.                                           
      *(4)                                                                      
           IF TIME-WORK-4 > 0                                                   
               COMPUTE YEAR = YEAR + TIME-WORK-4.                               
      *(5)                                                                      
           DIVIDE TIME-WORK-5 BY 1-DAY GIVING TIME-WORK-6                       
               REMAINDER TIME-WORK-7.                                           
           MOVE TIME-WORK-6 TO DAYS.                                            
      *(6)                                                                      
           DIVIDE TIME-WORK-7 BY 1-MINUTE GIVING TIME-WORK-8                    
               REMAINDER TIME-WORK-9.                                           
           DIVIDE TIME-WORK-8 BY 60 GIVING HOURS                                
               REMAINDER MINUTES.                                               
      *(7)                                                                      
           DIVIDE TIME-WORK-9 BY 1-SECOND GIVING SECONDS.                       
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
      *                                                                         
       CICS-STATISTICS.                                                         
           CALL  'SMF110' USING INPUT-RECORD,                                   
                                CICS-TRAN-TABLE.                                
                                                                                
       PRINT-CICS-TRANS-HEADER.                                                 
           WRITE OUTPUT-RECORD FROM OUTPUT-100.                                 
           WRITE OUTPUT-RECORD FROM OUTPUT-101.                                 
                                                                                
       PRINT-CICS-TRANS-DETAIL.                                                 
           MOVE CORRESPONDING CICS-TRANS(S-SUB)                                 
                TO OUTPUT-102.                                                  
           WRITE OUTPUT-RECORD FROM OUTPUT-102.                                 
           ADD 1 TO S-SUB.                                                      
