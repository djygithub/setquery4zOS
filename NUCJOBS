//IMSGEN02 JOB ,                                                                
// IMS,TYPRUN=HOLD,                                                             
// MSGCLASS=A                                                                   
//STEP1    EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSUDT0I)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
UDT0     TITLE 'DEVICE CHARACTERISTICS TABLE--DFSUDT0I'                         
         COPY  DVCTBL                                                           
DFSUDT0I CSECT      DEVICE CHARACTERISTICS TABLE                                
         DC    CL8'DFSUDT0I' MODULE NAME                                        
         DC    H'1'         NUMBER OF ENTRIES                                   
         DC    AL2(DVCENDA-DVC3270A)   LENGTH OF ENTRY                          
         DC    A(DVC1)        ADDRESS OF FIRST ENTRY                            
DVC1     DS    0H                                                               
         DC    CL8'3270-A02' SYMBOLIC NAME                                      
         DC    H'24'         LINES                                              
         DC    H'80'         COLUMNS                                            
         DC    XL4'04A00000'     FEATURES                                       
         END                                                                    
//STEP2    EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSOPL0I)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
DFSIOPL0 CSECT                                                                  
         DC    A(OPTLIST)                                                       
         ADFS  NOBRANCH                                                         
OPTLIST  DS    0F                                                               
         DC    CL4'SCD '                                                        
         DC    V(DFSNEP1I)                                                      
         DC    CL4'ESCD'                                                        
         DC    V(DFSNEP3I)                                                      
         DC    CL4'FRB '                                                        
         DC    V(DFSNEP5I)                                                      
         DC    CL4'    '                                                        
         DC    F'0'                                                             
         END                                                                    
