   ENTRY   DSNZMSTR                                                     01120000
   NAME    DSNZPARM(R)                                                  01130000
//                                                                      01130001
//*                                                                     01130001
//*        UPDATE BOOTSTRAP DATA SET #1                                 01130002
//*        NOTE: RC=888 IF BSDS IS ALREADY MIGRATED                     01130003
