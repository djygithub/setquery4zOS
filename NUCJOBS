CLL1     TITLE 'DFSICLL1 - IMS COMMUNICATION CONTROL BLOCKS, MODULE 1.'         
         DFSCCBS 59,4                                                           
         TITLE 'DFSICLL1 - COMMUNICATION TERMINAL TABLES (CTT).'                
DFSICTT0 CSECT                                                                  
         ENTRY CTT0                                                             
CTT0     ICTTG OPT1=231,OPT2=4,DD=DFSDNSC0,TSND=TRCONSS,TREC=TRCONSS,  ;        
               BUFSZ=146,INLNG=129,NTRYL=0,LTC=51                               
         ENTRY CTT1                                                             
CTT1     ICTTG BUFSZ=55,NTRYL=0,IDLNL=0,                               ;        
               INLNG=10,IDLTAB=0,IDLLF=0,                              ;        
               LTC=15,OPT1=103,OPT2=32,                                ;        
               OPT3=0,OPT4=0,OPT5=0,                                   ;        
               OPT6=0,                                                 ;        
               DD=DFSDN150,                                            ;        
               TSND=TRPRTPN,                                           ;        
               TREC=0                                                           
         ENTRY CTT2                                                             
CTT2     ICTTG BUFSZ=168,NTRYL=0,IDLNL=0,                              ;        
               INLNG=32,IDLTAB=0,IDLLF=0,                              ;        
               LTC=15,OPT1=103,OPT2=32,                                ;        
               OPT3=0,OPT4=0,OPT5=0,                                   ;        
               OPT6=0,                                                 ;        
               DD=DFSDN150,                                            ;        
               TSND=0,                                                 ;        
               TREC=0                                                           
         ENTRY CTT3                                                             
CTT3     ICTTG BUFSZ=2138,NTRYL=0,IDLNL=0,                             ;        
               BUF01=138,BUF02=138,                                    ;        
               INLNG=0,IDLTAB=0,IDLLF=0,                               ;        
               LTC=19,OPT1=101,OPT2=6,                                 ;        
               OPT3=63,OPT4=160,OPT5=8,                                ;        
               OPT6=0,                                                 ;        
               DD=DFSDN190,                                            ;        
               TSND=TRCONSS,                                           ;        
               TREC=TRCONSS                                                     
         ENTRY CTT4                                                             
CTT4     ICTTG BUFSZ=2138,NTRYL=0,IDLNL=0,                             ;        
               BUF01=138,BUF02=138,                                    ;        
               INLNG=0,IDLTAB=0,IDLLF=0,                               ;        
               LTC=19,OPT1=69,OPT2=6,                                  ;        
               OPT3=63,OPT4=160,OPT5=8,                                ;        
               OPT6=0,                                                 ;        
               DD=DFSDN190,                                            ;        
               TSND=TRCONSS,                                           ;        
               TREC=TRCONSS                                                     
         ENTRY CTT5                                                             
CTT5     ICTTG BUFSZ=2138,NTRYL=6,IDLNL=0,                             ;        
               BUF01=138,BUF02=138,                                    ;        
               INLNG=0,IDLTAB=0,IDLLF=0,                               ;        
               LTC=19,OPT1=85,OPT2=22,                                 ;        
               OPT3=63,OPT4=160,OPT5=8,                                ;        
               OPT6=0,                                                 ;        
               DD=DFSDN190,                                            ;        
               TSND=TRCONSS,                                           ;        
               TREC=TRCONSS                                                     
         ENTRY DFSTFCTT                                                         
DFSTFCTT ICTTG OPT1=231,OPT2=0,DD=DFSTDDM0,TSND=TRCONSS,TREC=TRCONSS,  ;        
               BUFSZ=146,INLNG=129,NTRYL=0,LTC=20                               
         ROUTINE GEN                                                            
         TITLE 'DFSICLL1 - COMMUNICATION TRANSLATION TABLES.'                   
DFSITRT0 CSECT                                                                  
         COPY TRCONSS                                                           
         COPY TRPRTPN                                                           
         END                                                                    
//STEP9    EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSCLV0I)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
CLV0     TITLE 'DFSCLV00 - IMS VTAM CONTROL BLOCKS, MODULE 0'                   
         GBLB  &DFBESEC   ENHANCED SECURITY SPECIFIED                           
&DFBESEC SETB  0      FOR EXTENDED CTB                                          
DFSCLV00 CSECT                                                                  
         DFSPPRE FUNC=NDSECT  IPAGE PREFIX                                      
         DC    16H'0' IPAGE PREFIX BUFFER (BPRE=)                               
         DFSCLB  V,4,HM31T022,CT3,0                                             
CT3     DFSCTB V,120,CLB4,3,000000,40000B00,08,2A,                     ;        
               00000000,0,CRB1,CI1,0,00,00,00                                   
CRB1    DFSCRB 0000,80,00,CT3,,00000000,HM31T022                                
CI1     DFSCIB 42C7000000000000,1572944,00,V,00                                 
WA3    DFSDSL 0                                                                 
         DC    XL4'0' BUMP TO 32 BYTE BOUNDARY                                  
         DFSCLB  V,5,USER1,CT4,0                                                
CT4     DFSCTB V,160,CLB5,4,000000,00020B00,08,2A,                     ;        
               00000000,0,CRB2,CI2,256,00,00,00                                 
CRB2    DFSCRB 0000,80,00,CT4,,00000000,USER1                                   
CI2     DFSCIB 42C7000000000000,1572944,00,V,00                                 
WA4    DFSDSL 0                                                                 
         DC    XL4'0' BUMP TO 32 BYTE BOUNDARY                                  
         DFSCLB  V,6,USER2,CT5,0                                                
CT5     DFSCTB V,160,CLB6,5,000000,00020B00,08,2A,                     ;        
               00000000,0,CRB3,CI3,256,00,00,00                                 
CRB3    DFSCRB 0000,80,00,CT5,,00000000,USER2                                   
CI3     DFSCIB 42C7000000000000,1572944,00,V,00                                 
WA5    DFSDSL 0                                                                 
         DC    XL4'0' BUMP TO 32 BYTE BOUNDARY                                  
         DFSCLB  VEND,7,TPNS0001,CT6,0                                          
CT6     DFSCTB V,200,CLB7,6,000000,00020B00,08,2A,                     ;        
               00000000,0,CRB4,CI4,256,00,00,00                                 
CRB4    DFSCRB 0000,80,00,CT6,,00000000,TPNS0001                                
CI4     DFSCIB 02C7000000000000,1572944,00,V,00                                 
WA6    DFSDSL 0                                                                 
         DC    XL4'0' BUMP TO 32 BYTE BOUNDARY                                  
         END                                                                    
