//STEP12   EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSISDBI)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
SDB0     TITLE 'DFSISDBI - SECURITY DIRECTORY BLOCKS (SDB).'                    
DFSISDB0 CSECT                                                                  
         ENTRY DFSISDB                                                          
DFSISDB  DS    0D                                                               
         DC    A(SMBL-DFSISDB)    OFFSET TO SMB LIST.                           
         DC    A(DMDL-DFSISDB)    OFFSET TO DMD LIST.                           
         DC    A(PSBL-DFSISDB)    OFFSET TO PSB LIST.                           
SMBL     DC    A((SMBLE-SMBLL)/L'SMBLL,L'SMBLL)                                 
SMBLL        DS    0CL9                                                         
         DFSSMB SDB,ADDINV,0                                                    
         DFSSMB SDB,ADDPART,0                                                   
         DFSSMB SDB,CLOSE,0                                                     
         DFSSMB SDB,DISBURSE,0                                                  
         DFSSMB SDB,DLETINV,0                                                   
         DFSSMB SDB,DLETPART,0                                                  
         DFSSMB SDB,DSPALLI,0                                                   
         DFSSMB SDB,DSPINV,0                                                    
         DFSSMB SDB,FPSAMP1,1                                                   
         DFSSMB SDB,FPSAMP2,0                                                   
         DFSSMB SDB,IVTCV,0                                                     
         DFSSMB SDB,IVTFD,1                                                     
         DFSSMB SDB,IVTFM,1                                                     
         DFSSMB SDB,IVTNO,0                                                     
         DFSSMB SDB,IVTNV,0                                                     
         DFSSMB SDB,PART,0                                                      
         DFSSMB SDB,Q101,0                                                      
         DFSSMB SDB,Q103,0                                                      
         DFSSMB SDB,Q105,0                                                      
         DFSSMB SDB,Q501,0                                                      
         DFSSMB SDB,Q610,0                                                      
         DFSSDB SMB                                                             
SMBLE        EQU    *                                                           
DMDL     DC    A((DMDLE-DMDLL)/L'DMDLL,L'DMDLL)                                 
DMDLL        DS    0CL8                                                         
         DFSDMD SDB,DBFSAMD1                                                    
         DFSDMD SDB,DBFSAMD2                                                    
         DFSDMD SDB,DBFSAMD3                                                    
         DFSDMD SDB,DBFSAMD4                                                    
         DFSDMD SDB,DI21PART                                                    
         DFSDMD SDB,IVPDB1                                                      
         DFSDMD SDB,IVPDB1I                                                     
         DFSDMD SDB,IVPDB2                                                      
         DFSDMD SDB,IVPDB3                                                      
         DFSDMD SDB,IVPDB4                                                      
         DFSSDB DMD                                                             
DMDLE        EQU    *                                                           
PSBL     DC    A((PSBLE-PSBLL)/L'PSBLL,L'PSBLL)                                 
PSBLL        DS    0CL8                                                         
         DFSPSBD SDB,DBF#FPU0                                                   
         DFSPSBD SDB,DBFSAMP1                                                   
         DFSPSBD SDB,DBFSAMP2                                                   
         DFSPSBD SDB,DBFSAMP3                                                   
         DFSPSBD SDB,DBFSAMP4                                                   
         DFSPSBD SDB,DBFSAMP5                                                   
         DFSPSBD SDB,DBFSAMP6                                                   
         DFSPSBD SDB,DFHSAM04                                                   
         DFSPSBD SDB,DFHSAM05                                                   
         DFSPSBD SDB,DFHSAM14                                                   
         DFSPSBD SDB,DFHSAM15                                                   
         DFSPSBD SDB,DFHSAM24                                                   
         DFSPSBD SDB,DFHSAM25                                                   
         DFSPSBD SDB,DFSIVPA                                                    
         DFSPSBD SDB,DFSIVPB                                                    
         DFSPSBD SDB,DFSIVPC                                                    
         DFSPSBD SDB,DFSIVP1                                                    
         DFSPSBD SDB,DFSIVP2                                                    
         DFSPSBD SDB,DFSIVP3                                                    
         DFSPSBD SDB,DFSIVP4                                                    
         DFSPSBD SDB,DFSIVP5                                                    
         DFSPSBD SDB,DFSIVP6                                                    
         DFSPSBD SDB,DFSIVP7                                                    
         DFSPSBD SDB,DFSIVP8                                                    
         DFSPSBD SDB,DFSIVP9                                                    
         DFSPSBD SDB,DFSSAM01                                                   
         DFSPSBD SDB,DFSSAM02                                                   
         DFSPSBD SDB,DFSSAM03                                                   
         DFSPSBD SDB,DFSSAM04                                                   
         DFSPSBD SDB,DFSSAM05                                                   
         DFSPSBD SDB,DFSSAM06                                                   
         DFSPSBD SDB,DFSSAM07                                                   
         DFSPSBD SDB,DFSSAM08                                                   
         DFSPSBD SDB,DFSSAM09                                                   
         DFSPSBD SDB,IM2Q101                                                    
         DFSPSBD SDB,IM2Q103                                                    
         DFSPSBD SDB,IM2Q105                                                    
         DFSPSBD SDB,IM2Q501                                                    
         DFSPSBD SDB,IM2Q610                                                    
         DFSSDB PSB                                                             
PSBLE        EQU    *                                                           
         END                                                                    
//STEP13   EXEC PGM=IEV90,REGION=4096K,                                         
//             PARM='LOAD,NODECK,NOLIST'                                        
//SYSLIB   DD DSN=IMS310.OPTIONS,DISP=SHR                                       
//         DD DSN=IMS310.GENLIB,DISP=SHR                                        
//         DD DSN=IMS310.GENLIBA,DISP=SHR                                       
//         DD DSN=IMS310.GENLIBB,DISP=SHR                                       
//         DD DSN=SYS1.MACLIB,DISP=SHR                                          
//         DD DSN=SYS1.AMODGEN,DISP=SHR                                         
//SYSLIN   DD DISP=OLD,                                                         
//      DSN=IMS310.OBJDSET(DFSISDCI)                                            
//SYSPRINT DD  SYSOUT=A,                                                        
//             DCB=(BLKSIZE=605),                                               
//             SPACE=(605,(100,50),RLSE,,ROUND)                                 
//SYSUT1   DD  UNIT=3390,DISP=(,DELETE),                                        
//             SPACE=(CYL,(15,15))                                              
//SYSIN    DD   *                                                               
SDC0     TITLE 'DFSISDCI - SECURITY DIRECTORY BLOCKS - DC (SDB).'               
DFSISDC0 CSECT                                                                  
         ENTRY DFSISDC                                                          
DFSISDC  DS    0D                                                               
         DC    A(CNTL-DFSISDC)    OFFSET TO CNT LIST.                           
         DC    A(CVBL-DFSISDC)    OFFSET TO CVB LIST.                           
         DC    AL1(12*0)    CTB EXTENSION.                                      
         DC    AL3(CTBL-DFSISDC)  OFFSET TO CTB LIST.                           
         DC    A(MASTER-DFSISDC)  OFFSET TO MASTER CNT.                         
         DC    A((CTBL-DFSISDC)+(3*6)+8)     OFFSET MASTER CTB.                 
         DC    A(VNML-DFSISDC)    OFFSET TO VTAM NODE LIST.                     
CNTL     DC    A((CNTLE-CNTLL)/L'CNTLL,L'CNTLL)                                 
CNTLL        DS    0CL8                                                         
         DFSCNT SDB,DFSMTCNT                                                    
         DFSCNT SDB,DFSRMCNT                                                    
         DFSCNT SDB,DFSTCF                                                      
         DFSCNT SDB,DFSTCFI                                                     
         DFSCNT SDB,HM31T021                                                    
MASTER   DFSCNT SDB,HM31T022                                                    
         DFSCNT SDB,HOWARD                                                      
         DFSCNT SDB,IVPPRT1                                                     
         DFSCNT SDB,IVPSPL1                                                     
         DFSCNT SDB,TPNS0001                                                    
         DFSCNT SDB,USER1                                                       
         DFSCNT SDB,USER2                                                       
         DFSCNT SDB,WTOR                                                        
         DFSSDB CNT                                                             
CNTLE        EQU    *                                                           
CVBL     DC    A((CVBLE-CVBLL)/L'CVBLL,L'CVBLL)                                 
CVBLL        DS    0CL11                                                        
         DC    CL10'ACTIVATE',X'03'                                             
         DC    CL10'ASSIGN',X'03'                                               
         DC    CL10'BROADCAST',X'01'                                            
         DC    CL10'CANCEL',X'00'                                               
         DC    CL10'CHANGE',X'03'                                               
         DC    CL10'CHECKPOINT',X'03'                                           
         DC    CL10'CLSDST',X'03'                                               
         DC    CL10'COMPT',X'03'                                                
         DC    CL10'DBDUMP',X'03'                                               
         DC    CL10'DBRECOVERY',X'03'                                           
         DC    CL10'DELETE',X'03'                                               
         DC    CL10'DEQUEUE',X'03'                                              
         DC    CL10'DISPLAY',X'03'                                              
         DC    CL10'END',X'01'                                                  
         DC    CL10'ERESTART',X'02'                                             
         DC    CL10'EXCLUSIVE',X'01'                                            
         DC    CL10'EXIT',X'01'                                                 
         DC    CL10'FORMAT',X'01'                                               
         DC    CL10'HOLD',X'00'                                                 
         DC    CL10'IDLE',X'03'                                                 
         DC    CL10'LOCK',X'01'                                                 
         DC    CL10'LOG',X'01'                                                  
         DC    CL10'LOOPTEST',X'01'                                             
         DC    CL10'MODIFY',X'02'                                               
         DC    CL10'NRESTART',X'02'                                             
         DC    CL10'OPNDST',X'03'                                               
         DC    CL10'PSTOP',X'03'                                                
         DC    CL10'PURGE',X'03'                                                
         DC    CL10'RCLSDST',X'00'                                              
