  ENTRY DFSIOPL0                                                                
  NAME DFSVNUCI(R)   IMS ON-LINE VIRTUAL NUCLEUS.                               
//IMSGEN06 JOB ,                                                                
// IMS,TYPRUN=HOLD,                                                             
// MSGCLASS=A                                                                   
//JOBLIB DD DSN=IMS310.RESLIB,DISP=SHR                                          
//S1 EXEC PGM=DFSUPAA0,REGION=360K,                                             
// PARM='NOXREF,NOCOMP,NOSUBS,NODIAG,LINECNT=55,STOPRC=8,DEVCHAR=I'             
//SYSLIB DD DSN=IMS310.GENLIBB,DISP=SHR                                         
//       DD DSN=IMS310.GENLIBA,DISP=SHR                                         
//REFIN DD DSN=IMS310.REFERAL,DISP=OLD                                          
//REFOUT DD DSN=IMS310.REFERAL,DISP=OLD                                         
//REFRD DD DSN=IMS310.REFERAL,DISP=OLD                                          
//SYSTEXT DD DSN=&&TXTPASS,                                                     
// UNIT=3390,SPACE=(CYL,(1,1)),DCB=BLKSIZE=800                                  
//SYSPRINT DD SYSOUT=A,                                                         
// DCB=(RECFM=FBA,LRECL=133,BLKSIZE=1330)                                       
//SYSUDUMP DD SYSOUT=A                                                          
//SEQBLKS DD DSN=&&BLKS,DISP=(NEW,PASS),                                        
// UNIT=3390,SPACE=(CYL,(1,1))                                                  
//SYSIN DD *                                                                    
         COPY MFSSM1                                                            
         COPY MFSSM2                                                            
DEVICE   EQU   '(3270,2)'                                                       
         COPY  MFSSM2A                                                          
DEVICE   EQU   '3270-A02'                                                       
         COPY  MFSSM2A                                                          
         FMTEND                                                                 
         COPY MFSSF1                                                            
         COPY MFSSFA1                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               DSCA=X'00C0',                         ERASE WITH WRITE  C        
               PFK=(PFKIN,1='/FOR ',11=NEXTMSGP)     DEFINE PFKS                
         DIV   TYPE=INOUT                                                       
         DPAGE CURSOR=((3,2))                                                   
OUTLINE  DFLD  POS=(1,2),LTH=131,ATTR=(PROT,NUM)                                
IN1      DFLD  POS=(3,2),LTH=131                                                
IN2      DFLD  POS=(5,2),LTH=131                                                
         COPY MFSSF2                                                            
         COPY MFSSFA2                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               SYSMSG=OUTL01,                                          C        
               DSCA=X'00C0',                         ERASE WITH WRITE  C        
               PFK=(PFKIN,1='/FOR ',11=NEXTMSGP)     DEFINE PFKS                
* ONLY 79 BYTES PER FIELD                                                       
         DIV   TYPE=INOUT                                                       
         DPAGE CURSOR=((1,2))                                                   
IN1      DFLD   POS=(1,2,1),LTH=79                                              
IN2      DFLD   POS=(2,2,1),LTH=79                                              
OUTL01   DFLD   POS=(3,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL02   DFLD   POS=(4,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL03   DFLD   POS=(5,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL04   DFLD   POS=(6,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL05   DFLD   POS=(7,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL06   DFLD   POS=(8,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL07   DFLD   POS=(9,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL08   DFLD   POS=(10,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL09   DFLD   POS=(11,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL10   DFLD   POS=(12,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL11   DFLD   POS=(13,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL12   DFLD   POS=(14,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL13   DFLD   POS=(15,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL14   DFLD   POS=(16,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL15   DFLD   POS=(17,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL16   DFLD   POS=(18,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL17   DFLD   POS=(19,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL18   DFLD   POS=(20,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL19   DFLD   POS=(21,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL20   DFLD   POS=(22,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL21   DFLD   POS=(23,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL22   DFLD   POS=(24,2,1),LTH=79,ATTR=(NUM,PROT)                             
         COPY MFSSF4                                                            
         COPY MFSSFA4                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               DSCA=X'00A0',                     ERASE ALL UNPROTECTED C        
               SYSMSG=DF01,                                            C        
               PFK=(PFKIN,1='/FOR ',11=NEXTMSGP)  PF KEYS                       
         DIV   TYPE=INOUT                                                       
         DPAGE CURSOR=((1,2))                                                   
DF01     DFLD  POS=(1,2,1),LTH=131                                              
DF02     DFLD  POS=(3,2,1),LTH=131                                              
DF03     DFLD  POS=(5,2,1),LTH=131                                              
DF04     DFLD  POS=(7,2,1),LTH=131                                              
         COPY MFSSFEND                                                          
//S2 EXEC PGM=DFSUNUB0,REGION=360K,                                             
// COND=(8,LT,S1)                                                               
//SEQBLKS DD DSN=&&BLKS,DISP=(OLD,DELETE)                                       
//UTPRINT DD SYSOUT=A,DCB=(RECFM=FBA,LRECL=133,BLKSIZE=1330)                    
//SYSUDUMP DD SYSOUT=A                                                          
//FORMAT DD DSN=IMS310.FORMAT,DISP=OLD                                          
//SYSPRINT DD SYSOUT=A                                                          
//SYSUT3 DD UNIT=3390,SPACE=(CYL,(1,1))                                         
//SYSUT4 DD UNIT=3390,SPACE=(CYL,(1,1))                                         
//DUMMY DD DUMMY,DCB=BLKSIZE=80                                                 
//S3 EXEC PGM=DFSUPAA0,REGION=360K,                                             
// PARM='NOXREF,NOCOMP,NOSUBS,NODIAG,LINECNT=55,STOPRC=8,DEVCHAR=I'             
//SYSLIB DD DSN=IMS310.GENLIBB,DISP=SHR                                         
//       DD DSN=IMS310.GENLIBA,DISP=SHR                                         
//REFIN DD DSN=IMS310.REFERAL,DISP=OLD                                          
//REFOUT DD DSN=&&TEMPPDS,DCB=IMS310.REFERAL,                                   
// SPACE=(CYL,(5,1,10)),UNIT=3390                                               
//REFRD DD DSN=*.REFOUT,VOL=REF=*.REFOUT,DISP=(OLD,DELETE)                      
//      DD DSN=IMS310.REFERAL,DISP=OLD                                          
//SYSTEXT DD DSN=&&TXTPASS,                                                     
// UNIT=3390,SPACE=(CYL,(1,1)),DCB=BLKSIZE=800                                  
//SYSPRINT DD SYSOUT=A,                                                         
// DCB=(RECFM=FBA,LRECL=133,BLKSIZE=1330)                                       
//SYSUDUMP DD SYSOUT=A                                                          
//SEQBLKS DD DSN=&&BLKS,DISP=(NEW,PASS),                                        
// UNIT=3390,SPACE=(CYL,(1,1))                                                  
//SYSIN DD *                                                                    
         COPY MFSSM2                                                            
DEVICE   EQU   '(3270,2)'                                                       
         COPY  MFSSM2A                                                          
DEVICE   EQU   '3270-A02'                                                       
         COPY  MFSSM2A                                                          
         FMTEND                                                                 
         COPY MFSSF1                                                            
         COPY MFSSFA1                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               DSCA=X'00C0',                         ERASE WITH WRITE  C        
               PFK=(PFKIN,1='/FOR ',11=NEXTMSGP)     DEFINE PFKS                
         DIV   TYPE=INOUT                                                       
         DPAGE CURSOR=((3,2))                                                   
OUTLINE  DFLD  POS=(1,2),LTH=131,ATTR=(PROT,NUM)                                
IN1      DFLD  POS=(3,2),LTH=131                                                
IN2      DFLD  POS=(5,2),LTH=131                                                
         COPY MFSSF2                                                            
         COPY MFSSFA2                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               SYSMSG=OUTL01,                                          C        
               DSCA=X'00C0',                         ERASE WITH WRITE  C        
               PFK=(PFKIN,1='/FOR ',11=NEXTMSGP)     DEFINE PFKS                
* ONLY 79 BYTES PER FIELD                                                       
         DIV   TYPE=INOUT                                                       
         DPAGE CURSOR=((1,2))                                                   
IN1      DFLD   POS=(1,2,1),LTH=79                                              
IN2      DFLD   POS=(2,2,1),LTH=79                                              
OUTL01   DFLD   POS=(3,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL02   DFLD   POS=(4,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL03   DFLD   POS=(5,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL04   DFLD   POS=(6,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL05   DFLD   POS=(7,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL06   DFLD   POS=(8,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL07   DFLD   POS=(9,2,1),LTH=79,ATTR=(NUM,PROT)                              
OUTL08   DFLD   POS=(10,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL09   DFLD   POS=(11,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL10   DFLD   POS=(12,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL11   DFLD   POS=(13,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL12   DFLD   POS=(14,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL13   DFLD   POS=(15,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL14   DFLD   POS=(16,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL15   DFLD   POS=(17,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL16   DFLD   POS=(18,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL17   DFLD   POS=(19,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL18   DFLD   POS=(20,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL19   DFLD   POS=(21,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL20   DFLD   POS=(22,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL21   DFLD   POS=(23,2,1),LTH=79,ATTR=(NUM,PROT)                             
OUTL22   DFLD   POS=(24,2,1),LTH=79,ATTR=(NUM,PROT)                             
         COPY MFSSF4                                                            
         COPY MFSSFA4                                                           
         DEV   TYPE=3270-A02,FEAT=IGNORE,                              C        
               DSCA=X'00A0',                     ERASE ALL UNPROTECTED C        
               SYSMSG=DF01,                                            C        
